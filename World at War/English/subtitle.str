REFERENCE           SEE1_IGD_005A_REZN_A
LANG_ENGLISH        "^1REZNOV: ^7Burn the wheatfields!"

REFERENCE           SEE1_IGD_006A_REZN_B
LANG_ENGLISH        "^1REZNOV: ^7Shoot!"

